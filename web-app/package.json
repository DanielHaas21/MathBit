    "@svgr/webpack": "^8.1.0",
    "@vitejs/plugin-react": "^5.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "create-remix": "^2.17.1",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.16.0",
    "i18next": "^25.6.0",
    "i18next-fs-backend": "^2.6.0",
    "openapi-fetch": "^0.9.7",
